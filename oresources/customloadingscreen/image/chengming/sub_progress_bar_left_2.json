{
    "parent": "builtin/image",
    "image": "customloadingscreen:image/chengming/sub_progress_bar.png",
    "position_type": "CENTER",
    "offset_pos": "CENTER",
    "position": {
        "x": "forge_progress_bar_count() > 3 ? -screen_width * 0.349 + forge_progress_bar_percent(3) * screen_width * 0.15 : 0",
        "y": "forge_progress_bar_count() > 3 ? screen_height * 0.15 + 70 : 0",
        "width": "forge_progress_bar_count() > 3 ? (screen_width * 0.3) * forge_progress_bar_percent(3) : 0",
        "height": "forge_progress_bar_count() > 3 ? 8 : 0"
    },
    "texture": {
        "x": 0,
        "y": 0,
        "width": "forge_progress_bar_percent(3)",
        "height": 1
    }
}
